{
  "name": "kross-hugo",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "author": "gethugothemes",
  "scripts": {
    "dev": "yarn tinacms dev -c 'hugo server -D -p 1313'",
    "test": "hugo server --disableFastRender --navigateToChanged --templateMetrics --templateMetricsHints --verbose --buildDrafts --buildExpired --buildFuture --watch --forceSyncStatic -e production --minify",
    "build": "hugo --gc --minify --templateMetrics --templateMetricsHints --verbose --buildDrafts --buildExpired --buildFuture --forceSyncStatic",
    "update": "hugo mod clean && hugo mod get -u ./... && hugo mod tidy"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@tinacms/cli": "^1.5.38",
    "@types/node": "^20.10.3",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.1",
    "prettier-plugin-go-template": "0.0.13"
  },
  "dependencies": {
    "tinacms": "^1.5.28"
  }
}
